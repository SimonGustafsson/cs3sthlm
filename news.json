---
layout: null
---
{"newsList":[{% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "author": "{{ post.author }}",
      "category": "{{ post.categories }}",
      "date": "{{ post.date | date_to_string }}",
      "content_html": {{ post.content | strip_newlines | jsonify }}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]}