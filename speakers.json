---
layout: null
---
{"speakerList":[{% for speaker in site.data.speakers %}
	{
		"id"		: "{{ speaker.id }}",
		"type"		: "{{ speaker.type }}",
		"name"		: "{{ speaker.name }}",
		"role"		: "{{ speaker.role }}",
		"work"		: "{{ speaker.work }}",
		"country"	: "{{ speaker.country }}",
		"shortcode" : "{{ speaker.shortcode }}",
		"image"		: "https://cs3sthlm.se/assets/speakers/{{ speaker.image }}",
		"bio"		: {{ speaker.bio | markdownify | strip_newlines | jsonify }},
		"date"		: "{{ speaker.date }}",
		"time"		: "{{ speaker.time }}",
		"place"		: "{{ speaker.place }}",
		"title"		: "{{ speaker.title }}",
		"description": {{ speaker.description | markdownify | strip_newlines | jsonify }}
	}{% unless forloop.last %},{% endunless %}
{% endfor %}
]}