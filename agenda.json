---
layout: 
---

{"agendaList":[{% for item in site.data.agenda %}
	{
		"id"		: "{{ item.id }}",
		"day"		: "{{ item.day }}",
		"type"		: "{{ item.type }}",
		"date"		: "{{ item.date }}",
		"time"		: "{{ item.time }}",
		"place" 	: "{{ item.place }}",
		"subject" 	: "{{ item.subject | strip_html }}",
		"author": {
		{% for item in item.author %}
		"name" 		: "{{ item.name }}",
		"role"		: "{{ item.role }}",
		"work"		: "{{ item.work }}",
		"image" 	: "https://cs3sthlm.se/assets/speakers/{{ item.image }}"{% unless forloop.last %},{% endunless %}
		{% endfor %}
		},
		"partner": {
		{% for item in item.partner %}
		"name" 		: "{{ item.name }}",
		"role"		: "{{ item.role }}",
		"work"		: "{{ item.work }}",
		"image" 	: "https://cs3sthlm.se/assets/speakers/{{ item.image }}"{% unless forloop.last %},{% endunless %}
		{% endfor %}
		},
		"description": {{ item.description | markdownify | strip_newlines | jsonify }}
	}{% unless forloop.last %},{% endunless %}
{% endfor %}
]}