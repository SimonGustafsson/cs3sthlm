---
layout: null
---
{"newsList":[{% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "category": "{{ post.categories }}",
      "date": "{{ post.date | date_to_string }}",
      "content": "{{ post.content | jsonify | strip_html | escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]}