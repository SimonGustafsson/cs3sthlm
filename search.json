---
layout: 
---

[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
      "path"     : "{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for page in site.pages %}
   {
     {% if page.title != nil %}
        "title"    : "{{ page.title | strip_html }}",
        "url"      : "{{ site.baseurl }}{{ page.url }}",
        "path"     : "{{ page.permalink }}"
     {% endif %}
   } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for speaker in site.data.speakers %}
   {
     {% if speaker.name != nil %}
        "title"    : "{{ speaker.name }}",
        "url"      : "{{ site.baseurl }}/{{ speaker.permalink }}",
        "path"     : "{{ speaker.permalink }}"
     {% endif %}
   } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]