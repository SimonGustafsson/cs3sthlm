---
layout: null
---
{"newsList":[{% for post in site.posts %}
    {
      "title"    : "{{ post.title | smartify | jsonify }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
      "path"     : "{{ post.url }}",
      "content"	 : "{{ post.content | jsonify | strip_html }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]}