---
layout: 
---

{"agendaList":[{% for item in site.data.agenda %}
	{
		"id"		: "{{ item.id }}",
		"day"		: "{{ item.day }}",
		"type"		: "{{ item.type }}",
		"time"		: "{{ item.time }}",
		"role"		: "{{ item.role }}",
		"headline"	: "{{ item.headline | strip_html }}",
		"speaker"	: "{{ item.speaker }}",
		"work"		: "{{ item.work }}",
		"extra"		: "{{ item.extra }}",
		"place"		: "{{ item.place }}"
	}{% unless forloop.last %},{% endunless %}
{% endfor %}
]}